/// <summary>
/// Returns a fixed color.
/// </summary>
/// <remarks>
/// TVALUE: generic float4 - the color (as a float4).
/// </remarks>
shader ComputeColorFixed<float4 TVALUE> : ComputeColor
{
    override float4 Compute()
    {
        return TVALUE;
    }
};
