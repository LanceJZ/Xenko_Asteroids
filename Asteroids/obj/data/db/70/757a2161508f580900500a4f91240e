namespace SiliconStudio.Xenko.Rendering.Materials
{
    class IMaterialCelShadingLightFunction : MaterialPixelShadingStream
    {
        float3 Compute(float lightIn)
        {
            return float3(lightIn, lightIn, lightIn);
        }
    };
}
