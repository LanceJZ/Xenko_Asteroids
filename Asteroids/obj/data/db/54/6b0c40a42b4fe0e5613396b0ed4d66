/// <summary>
/// Defines a stream with custom attribute (usually texcoord).
/// </summary>
/// <remarks>
/// NAME: generic Semantic - the name of the texcoord (e.g. TEXCOORD0).
/// </remarks>
shader DynamicTextureStream<Semantic NAME>
{
    stream float2 TexCoord : NAME;
};
